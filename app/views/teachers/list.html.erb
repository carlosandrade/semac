<h1>Listing Teachers</h1>

<table>
  <tr>
    <th>Registration</th>
    <th>Name</th>
  </tr>

<% @teachers.each do |teacher| %>
  <tr>

    <td><%=h teacher.registration %></td>
    <td><%= link_to teacher.name, :action => "show", :id => teacher %></td>
  </tr>
<% end %>
</table>
<br />
<%= link_to 'Back', :action => ".."%>
